new() {
  local v="$1"
  if [ -z "$v" ]; then
    echo "mod_new: missing args <subcommand>" 
  else
    new_$v "${@:2}"
  fi
}

new_mod() {
  local v="$1"
  local mod=$chk_bash_mods_path/$v.mod
  if [ -z "$v" ]; then
    echo "mod_new: missing args <bash_mod_name>"
  else
    vi $mod
  fi
}

new_window() {
  local v="$1"
  if [ -z "$v" ]; then
    echo "mod_new: missing args <window_name>"
  else
    tmux new-window -n $v
  fi
}