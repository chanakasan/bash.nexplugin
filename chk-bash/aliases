# debug
alias rr="source $HOME/.bashrc"
alias ed3="vi $HOME/.bashrc"
alias edx="vi $chk_bash_path/custom"

# safety
alias cp="cp -i"
alias mv="mv -i"
alias rm="rm -i"

# utils
alias cl="clear"
alias ll="ls -lah"
alias mkdir="mkdir -p"

# nav
alias ..="cd .."
alias cd..="cd .."
alias cd-="cd -"
alias cdd="cd -"