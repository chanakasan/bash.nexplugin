g() {
  local a="$1"
  if [ $a ]; then
    g-$a "${@:2}"
   else
    git status
  fi
}

ed() {
  local v1="${1:-a}"
  local v2="$2"
  local file=$chk_bash_path/$v1
  local tmux=$chk_dotfiles_path/chk-tmux/main.conf
  if [ -f "$file" ]; then
    vi $file
  elif [ "$v1" = "mod" ]; then
    ed_mod "$v2"
  elif [ "$v1" = "tmux" ]; then
    vi $tmux
  else
    ed_$v1
  fi
}
